{
  "name": "flickr-wormhole",
  "description": "A Serverless GraphQL to REST Gateway Wrapper for the Flickr API",
  "version": "0.0.0",
  "private": false,
  "license": "MIT",
  "author": "Drake Costa <drake@saeris.io> (http://saeris.github.io/)",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Saeris/Flickr-Wormhole.git"
  },
  "engines": {
    "node": "8.5.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development serverless offline start --dontPrintOutput",
    "start:prod": "cross-env NODE_ENV=production serverless --stage=production offline start --dontPrintOutput",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "cross-env NODE_ENV=development serverless --stage=dev deploy",
    "deploy:prod": "cross-env NODE_ENV=production serverless --stage=production deploy"
  },
  "dependencies": {
    "apollo-server-hapi": "^1.1.2",
    "babel-polyfill": "^7.0.0-beta.0",
    "dataloader": "^1.3.0",
    "date-fns": "^1.28.5",
    "good": "^7.3.0",
    "good-winston": "^4.0.0",
    "graphql": "^0.10.0",
    "graphql-custom-types": "^1.1.0",
    "graphql-iso-date": "^3.3.0",
    "graphql-relay": "^0.5.2",
    "graphql-tools": "^1.0.0",
    "hapi": "^16.6.2",
    "isomorphic-fetch": "^2.2.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.125.0",
    "babel-cli": "^7.0.0-beta.0",
    "babel-core": "^7.0.0-beta.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "2.0.0-beta.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "dotenv": "^4.0.0",
    "env-cmd": "^7.0.0",
    "eslint": "^4.8.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.37.0",
    "eslint-plugin-graphql": "^1.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.56.0",
    "graphql-tag": "^2.4.2",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.0",
    "serverless-domain-manager": "^1.1.12",
    "serverless-offline": "^3.15.3",
    "serverless-webpack": "^3.1.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
